{
    "transform": {
        "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "testPathIgnorePatterns":["<rootDir>/lib/", "<rootDir>/node_modules/"],
    "displayName":{
        "name": "FamillyGraph",
        "color": "blue"
      },
      "silent": false,
      "verbose":false,
      "coverageReporters":["lcov"],
      "collectCoverage":true

}